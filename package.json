{
  "name": "visa_ds-160_state_frontend",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages": "npm run pages:build && npm run pages:copy",
    "pages:build": "next build",
    "pages:copy": "rm -rf _site && mv out _site && cp -r public/*.svg _site/",
    "prettier": "prettier . --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@trussworks/react-uswds": "^9.1.0",
    "next": "^15.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{css,md}": "prettier --write"
  }
}
