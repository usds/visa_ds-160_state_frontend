    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "storybook": "^8.6.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{css,md}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
